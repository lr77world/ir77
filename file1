local success, result = true

local function executeScript()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/lr77world/ir77/main/startup"))()
end

local function handleError(error)
    print("An error occurred while running the script:")
    if type(error) == "string" then
        print("Error message: " .. error)
    else
        print("Error type: " .. type(error))
        print("Error details:")
        for key, value in pairs(error) do
            print(key .. ":", value)
        end
    end
end

local status, error = pcall(executeScript)

if status then
    print("Script executed successfully.")
else
    if error and error:find("attempt to call a nil value") then
        print("Error: Attempt to call a nil value. Please check your script for any missing function or variable assignments.")
    else
        handleError(error)
    end
end
