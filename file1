local success, result = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/lr77world/ir77/main/menu"))()
end)

if success then
    print("Script executed successfully.")
else
    print("An error occurred while running the script:")
    if type(result) == "string" then
        print("Error message: " .. result)
    else
        print("Error type: " .. type(result))
        print("Error details: ")
        for key, value in pairs(result) do
            print(key .. ":", value)
        end
    end
end
